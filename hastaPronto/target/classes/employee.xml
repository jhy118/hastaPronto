<beans xmlns="http://www.springframework.org/schema/beans" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="controller.employee.EmployeeController"/>
	<bean class="service.employee.EmployeeRegistService" id="employeeRegistService"/>
	<bean class="service.employee.EmployeeListService" id="employeeListService"/>
	<bean class="service.employee.EmployeeDetailService" id="employeeDetailService"/>
	<bean class="service.employee.EmployeeModifyService" id="employeeModifyService"/>
	<bean class="service.employee.EmployeeDeleteService" id="employeeDeleteService"/>
	<bean class="repository.employee.EmployeeRepository" id="employeeRepository"/>
</beans>