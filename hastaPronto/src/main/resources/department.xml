		xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="controller.department.DepartmentController"/>
	<bean class="service.department.DepartmentRegistService" id="departmentRegistService"/>
	<bean class="service.department.DepartmentListService" id="departmentListService"/>
