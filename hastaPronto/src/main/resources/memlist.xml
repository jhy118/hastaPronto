<beans xmlns="http://www.springframework.org/schema/beans" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="controller.member.MemberListController"/>
	<bean id = "memberListService" 
		class="service.list.MemberListService"/>
	<bean id = "memberListRepository" 
		class="repository.member.MemberListRepository"/>
	<bean id = "memberDetailService" 
		class="service.list.MemberDetailService"/>
	<bean id = "memberModifyService" 
		class="service.list.MemberModifyService"/>
	<bean id = "memberDeleteService" 
		class="service.list.MemberDeleteService"/>
</beans>